{
    "options": {
        "plugins": [{
            "removeViewBox": false
        },{
            "removeUselessStrokeAndFill": false
        }]
    },
    "dist": {
        "files": [{
            "expand": true,
            "cwd": "<%= path.build.dist %><%=path.assets %>/img/",
            "src": "**/*.svg",
            "dest": "<%= path.build.dist %><%=path.assets %>/img/"
        }]
    }
}
